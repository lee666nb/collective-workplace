.container {
  padding: 20rpx;
}

.header {
  text-align: center;
  margin-bottom: 40rpx;
}

.title {
  font-size: 36rpx;
  font-weight: bold;
}

.chart-container {
  margin-bottom: 40rpx;
}

.main {
  display: flex;
  flex-direction: column;
  gap: 30rpx;
}

.feature-card {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 150rpx;
  background-color: #f5f5f5;
  border-radius: 20rpx;
  box-shadow: 0 10rpx 20rpx rgba(0, 0, 0, 0.1);
  cursor: pointer;
}

.card-title {
  font-size: 30rpx;
  color: #333;
}
