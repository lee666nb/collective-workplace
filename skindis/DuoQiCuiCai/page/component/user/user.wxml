<view class="form-container">
    <text class="title">Welcome back</text>
    <form class="form">
        <input type="text" class="input" placeholder="Email" />
        <input type="password" class="input" placeholder="Password" />
        <text class="page-link">
            <text class="page-link-label">Forgot Password?</text>
        </text>
        <button form-type="submit" class="form-btn">Log in</button>
    </form>
    <text class="sign-up-label">
        Don't have an account?<text class="sign-up-link">Sign up</text>
    </text>
    <view class="buttons-container">
        <view class="apple-login-button">
            <icon type="success" class="apple-icon" />
            <text>Log in with Apple</text>
        </view>
        <view class="google-login-button">
            <icon type="success" class="google-icon" />
            <text>Log in with Google</text>
        </view>
    </view>
</view>


